		"format": "biome format . --fix"
	},
	"devDependencies": {
		"@types/bun": "latest"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"grammy": "^1.38.4",
		"zod": "^4.1.13"
	}
}
